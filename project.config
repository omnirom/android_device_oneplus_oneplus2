[access]
	inheritFrom = All-Projects
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group OnePlus 2 Maintainers
	label-Verified = -1..+1 group OnePlus 2 Maintainers
	submit = group OnePlus 2 Maintainers
	create = group OnePlus 2 Maintainers
	push = +force group OnePlus 2 Maintainers
[access "refs/for/refs/heads/*"]
	pushMerge = group OnePlus 2 Maintainers
